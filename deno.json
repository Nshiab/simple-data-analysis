{
  "name": "@nshiab/simple-data-analysis",
  "version": "3.13.0",
  "exports": "./src/index.ts",
  "tasks": {
    "all-tests": "deno install --allow-scripts=npm:duckdb && deno fmt --check && deno lint && deno check src/index.ts && deno publish --allow-dirty --dry-run && deno test -A --fail-fast",
    "publish": "deno task all-tests && deno publish"
  },
  "publish": {
    "exclude": [
      "test",
      "docs",
      ".github",
      ".eslintignore",
      ".eslintrc.cjs",
      ".mocharc.json",
      ".prettierignore",
      ".prettierrc",
      "package.json",
      "syncJsrVersion.js",
      "package-lock.json",
      "rollup.config.js",
      "tsconfig.json",
      "typedoc.json"
    ]
  },
  "nodeModulesDir": "auto",
  "imports": {
    "@duckdb/duckdb-wasm": "npm:@duckdb/duckdb-wasm@^1.29.0",
    "@nshiab/journalism": "jsr:@nshiab/journalism@^1.18.10",
    "@std/assert": "jsr:@std/assert@^1.0.6",
    "apache-arrow": "npm:apache-arrow@^17.0.0",
    "duckdb": "npm:duckdb@1.0.0"
  },
  "fmt": {
    "exclude": [".sda-cache", "test/output"]
  }
}
