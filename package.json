{
    "name": "simple-data-analysis",
    "version": "0.9.0",
    "description": "Easy to use library for most common data analysis tasks.",
    "homepage": "https://github.com/nshiab/simple-data-analysis#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nshiab/simple-data-analysis.git"
    },
    "keywords": [
        "data",
        "analysis",
        "visualization"
    ],
    "license": "MIT",
    "author": {
        "name": "Nael Shiab",
        "email": "nael.shiab@gmail.com",
        "url": "http://naelshiab.com"
    },
    "type": "module",
    "files": [
        "dist/",
        "src/"
    ],
    "module": "dist/index.js",
    "main": "dist/index.js",
    "jsdelivr": "dist/simple-data-analysis.min.js",
    "unpkg": "dist/simple-data-analysis.min.js",
    "exports": {
        "umd": "./dist/simple-data-analysis.min.js",
        "default": "./dist/index.js"
    },
    "scripts": {
        "test": "mocha --require ts-node/register test/unit/**/*.test.ts",
        "test-doc": "node --loader ts-node/esm test/integration/SimpleDocument.test.tsx",
        "test-data": "node --loader ts-node/esm test/integration/SimpleData.test.ts",
        "test-data-node": "node --loader ts-node/esm test/integration/SimpleDataNode.test.ts",
        "lint": "eslint . --ext .ts",
        "format": "prettier --ignore-path .prettierignore --write './**/*.ts'",
        "check-format": "prettier --ignore-path .prettierignore --check './**/*.ts'",
        "allTests": "npm run format && npm run test && npm run test-data && npm run test-data-node && npm run test-doc && npm run lint",
        "build": "npm run allTests && rm -rf dist && npx tsc && rollup --config"
    },
    "engines": {
        "node": ">=16.0"
    },
    "bugs": {
        "url": "https://github.com/nshiab/simple-data-analysis/issues"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^22.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.2.1",
        "@types/d3-array": "^3.0.2",
        "@types/d3-scale": "^4.0.2",
        "@types/d3-time-format": "^4.0.0",
        "@types/jsdom": "^16.2.14",
        "@types/lodash.camelcase": "^4.3.7",
        "@types/lodash.clonedeep": "^4.5.7",
        "@types/lodash.isequal": "^4.5.6",
        "@types/mocha": "^9.1.1",
        "@types/papaparse": "^5.3.2",
        "@typescript-eslint/eslint-plugin": "^5.23.0",
        "@typescript-eslint/parser": "^5.23.0",
        "eslint": "^8.15.0",
        "eslint-config-prettier": "^8.5.0",
        "mocha": "^9.2.2",
        "prettier": "^2.6.2",
        "rollup": "^2.70.2",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-visualizer": "^5.6.0",
        "simple-data-analysis": "^0.8.0",
        "tempy": "^3.0.0",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.4"
    },
    "dependencies": {
        "@emotion/react": "^11.9.0",
        "@emotion/server": "^11.4.0",
        "@emotion/styled": "^11.8.1",
        "@mui/icons-material": "^5.8.0",
        "@mui/material": "^5.8.0",
        "@observablehq/plot": "^0.4.3",
        "@types/react": "^18.0.9",
        "@types/react-dom": "^18.0.4",
        "axios": "^0.27.2",
        "d3-array": "^3.1.6",
        "d3-scale": "^4.0.2",
        "d3-time-format": "^4.1.0",
        "jsdom": "^19.0.0",
        "lodash.camelcase": "^4.3.0",
        "lodash.clonedeep": "^4.5.0",
        "lodash.isequal": "^4.5.0",
        "papaparse": "^5.3.2",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "simple-statistics": "^7.7.5"
    }
}
