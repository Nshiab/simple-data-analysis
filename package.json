{
    "name": "simple-data-analysis",
    "version": "0.30.0",
    "description": "Easy to use library for most common data analysis tasks.",
    "homepage": "https://github.com/nshiab/simple-data-analysis#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nshiab/simple-data-analysis.git"
    },
    "keywords": [
        "data",
        "analysis",
        "visualization",
        "nodejs",
        "javascript"
    ],
    "license": "MIT",
    "author": {
        "name": "Nael Shiab",
        "email": "nael.shiab@gmail.com",
        "url": "http://naelshiab.com"
    },
    "type": "module",
    "files": [
        "dist/",
        "src/"
    ],
    "module": "dist/index.js",
    "main": "dist/index.js",
    "jsdelivr": "dist/simple-data-analysis.min.js",
    "unpkg": "dist/simple-data-analysis.min.js",
    "exports": {
        "umd": "./dist/simple-data-analysis.min.js",
        "node": "./dist/index.js",
        "default": "./dist/indexWeb.js"
    },
    "scripts": {
        "test": "mocha --require ts-node/register \"test/unit/**/*.test.ts\"",
        "test-simple-data": "node --experimental-json-modules --loader ts-node/esm test/integration/SimpleData.test.ts",
        "test-simple-data-node": "node --loader ts-node/esm test/integration/SimpleDataNode.test.ts",
        "test-simple-data-doc": "node --loader ts-node/esm test/integration/SimpleDocument.test.tsx",
        "test-build": "rm -rf dist && npx tsc && rollup --config",
        "allTests": "npm run format && npm run test && npm run test-simple-data noLogs && npm run test-simple-data-node noLogs && npm run test-simple-data-doc noLogs && npm run test-build && npm run lint",
        "build": "npm run allTests && npx tsc && npm run commit-doc",
        "doc": "npx typedoc --name Simple-data-analysis --includeVersion --readme none --out docs src/index.ts",
        "commit-doc": "git checkout main && npm run doc && git add docs/ && git commit -m 'update documentation' && git push",
        "lint": "eslint . --ext .ts",
        "format": "prettier --ignore-path .prettierignore --write './**/*.ts'",
        "check-format": "prettier --ignore-path .prettierignore --check './**/*.ts'"
    },
    "engines": {
        "node": ">=16.0"
    },
    "bugs": {
        "url": "https://github.com/nshiab/simple-data-analysis/issues"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^22.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.2.1",
        "@rollup/plugin-typescript": "^8.3.2",
        "@types/d3-array": "^3.0.2",
        "@types/d3-dsv": "^3.0.0",
        "@types/d3-fetch": "^3.0.1",
        "@types/d3-scale": "^4.0.2",
        "@types/d3-time-format": "^4.0.0",
        "@types/jsdom": "^16.2.14",
        "@types/json2csv": "^5.0.3",
        "@types/lodash.camelcase": "^4.3.7",
        "@types/lodash.clonedeep": "^4.5.7",
        "@types/lodash.isequal": "^4.5.6",
        "@types/lodash.uniqby": "^4.7.7",
        "@types/lodash.uniqwith": "^4.5.7",
        "@types/mocha": "^9.1.1",
        "@types/react": "^18.0.9",
        "@types/react-dom": "^18.0.4",
        "@types/sinon": "^10.0.11",
        "@typescript-eslint/eslint-plugin": "^5.23.0",
        "@typescript-eslint/parser": "^5.23.0",
        "eslint": "^8.15.0",
        "eslint-config-prettier": "^8.5.0",
        "mocha": "^10.0.0",
        "prettier": "^2.6.2",
        "rollup": "^2.70.2",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-visualizer": "^5.6.0",
        "sinon": "^14.0.0",
        "tempy": "^3.0.0",
        "ts-node": "^10.7.0",
        "tslib": "^2.4.0",
        "typedoc": "^0.23.5",
        "typescript": "^4.6.4"
    },
    "dependencies": {
        "@emotion/react": "^11.9.0",
        "@emotion/server": "^11.4.0",
        "@emotion/styled": "^11.8.1",
        "@mui/icons-material": "^5.8.0",
        "@mui/material": "^5.8.0",
        "@observablehq/plot": "^0.5.2",
        "axios": "^0.27.2",
        "d3-array": "^3.1.6",
        "d3-dsv": "^3.0.1",
        "d3-fetch": "^3.0.1",
        "d3-regression": "^1.3.10",
        "d3-scale": "^4.0.2",
        "d3-time-format": "^4.1.0",
        "jsdom": "^20.0.0",
        "json2csv": "^5.0.7",
        "lodash.camelcase": "^4.3.0",
        "lodash.clonedeep": "^4.5.0",
        "lodash.isequal": "^4.5.0",
        "lodash.uniqby": "^4.7.0",
        "lodash.uniqwith": "^4.5.0",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "simple-statistics": "^7.7.5"
    },
    "browser": {
        "net": false
    }
}
